Transform: S3Objects

Parameters:
  S3BucketRef:
    Description:  A reference to the S3 bucket created in the service stack
    Type: String
    Default: com-github-dstaflund-udagram-s3-bucket

Resources:
  ZippedTestFile:
    Type: AWS::S3::Object
    Properties:
      Target:
        Bucket: !Sub ${S3BucketRef}
        Key: working-test.zip
        ACL: public-read
        ContentType: application/zip
      Base64Body: 
